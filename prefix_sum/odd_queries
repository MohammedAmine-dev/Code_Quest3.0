#include <bits/stdc++.h>
using namespace std;
void solve();
int main()
{
    long long t;
    cin>>t;
    while(t--)
    {
        solve();
    }

}
void solve()
{
    long long n,q;
    cin>>n>>q;
    vector<long long> arr(n);
    vector<long long> prefix(n+1,0);
    for (int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for (int i=1;i<=n;i++)
    {
        prefix[i]=prefix[i-1]+arr[i-1];
    }

    long long l,r,k;
    while (q--)
    {
        cin>>l>>r>>k;
        if ((prefix.back()-prefix[r]+prefix[l-1]-prefix[0]+((r-l+1)*k))%2==1)
        {
            cout << "YES"<<endl;
        }
        else{
            cout << "NO"<<endl;
        }


}}