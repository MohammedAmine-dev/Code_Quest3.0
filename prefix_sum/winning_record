#include <bits/stdc++.h>
using namespace std;
int main(){
long long n,k;
cin>>n>>k;
string s;
cin>>s;
int nb=0;
vector<int> pr(n+1,0);
for (int i=1;i<=n;i++)
{
    if (s[i-1]=='W')
    {
        pr[i]=++nb;
    }
    else{
        pr[i]=pr[i-1];    }
}

while (k--)
{
    long long i,j;
    cin>> i>>j;
    if(pr[j]-pr[i-1]>((j-i+1)/2)) cout<<'Y';
    else cout <<'N';
     
}
}