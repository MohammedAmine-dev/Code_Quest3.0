#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    long long l,r;
    cin>>l>>r;
    vector<long long>arr(r-l+1);
    for (int i=0;i<r-l+1;i++)
    {
        arr[i]=l++;
    }
    int op=0;
    while(arr[0]!=0) 
    {
        arr[0]=int(arr[0]/3);
        op++;}
    for(int i=1;i<arr.size()-1;i++)
    {
        while(arr[i]!=0)
        {
            arr[i]=int(arr[i]/3);
            op++;
            arr[i-1]*=3;
        }
    }
    //COMPETITVE PROGAMMING HELPER EXTENSION
    while(arr.back()!=0) 
    {
        arr[arr.size()-1]=int(arr[arr.size()-1]/3);
        op++;}
    
        cout <<op<<endl;




}