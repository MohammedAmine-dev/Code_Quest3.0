#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    
    cin>>t;
    while (t--)
    {
        long long n;
        cin>>n;
        long long k;
        cin>>k;vector<long long > arr(k+1);
        vector<long long > res(n);
        long long i=0;
        arr[0]=0;
        for (i=1;i<=k;i++)
        {
            cin>>arr[i];
        }
        for (long long i=1;i<=k;i++)
        {
            res[i]=arr[i]-arr[i-1];
        }
        int s=0;
        for (int i = 2; i <k; i++) {
        if (res[i] < res[i-1]) s=1;
    }
    if (s==0) cout <<"YES"<<endl;
    else {
        cout<<"NO"<<endl;
    }
    }
}