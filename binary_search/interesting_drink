#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector <int>shops(n);
    for (int i=0;i<n;i++)
    {
        cin>>shops[i];
    }
    int q;
    cin>>q;
    sort(shops.begin(),shops.end());
    
    while(q--)
    {
        long long m;
        cin>>m;
        int idx = upper_bound(shops.begin(), shops.end(), m) - shops.begin();
       cout<<idx<<endl;
    }

}